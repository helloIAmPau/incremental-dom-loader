{
  "name": "incremental-dom-loader",
  "version": "0.0.5",
  "description": "An incremental-dom loader for webpack. It compiles an HTML file into an incremental-dom script",
  "main": "index.js",
  "scripts": {
    "examples": "webpack --config example/webpack.config.js",
    "develop": "nodemon --watch 'lib/**/*.js' --watch 'test/**/*.test.js' --watch index.js --exec npm test",
    "test": "eslint index.js test/ lib/ && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloiampau/incremental-dom-loader.git"
  },
