  "bugs": {
    "url": "https://github.com/helloiampau/incremental-dom-loader/issues"
  },
  "homepage": "https://github.com/helloiampau/incremental-dom-loader#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "incremental-dom": "^0.5.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "sandboxed-module": "^2.0.3",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "htmlparser2": "^3.9.2"
  }
